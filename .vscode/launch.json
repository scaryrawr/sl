{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'sl'",
      "cargo": {
        "args": [
          "build",
          "--bin=sl",
          "--package=sl"
        ],
        "filter": {
          "name": "sl",
          "kind": "bin"
        }
      },
      "args": [
        "-a"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'sl'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=sl",
          "--package=sl"
        ],
        "filter": {
          "name": "sl",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "msedge",
      "name": "http://127.0.0.1:3000/apps/page/lib/index.html",
      "request": "launch",
      "url": "http://127.0.0.1:3000/apps/page/lib/index.html",
      "pathMapping": {
        "/": "${workspaceFolder}/apps/page/lib"
      }
    }
  ]
}