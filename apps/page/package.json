{
  "name": "page",
  "version": "1.0.0",
  "description": "Demo page of sl",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "bun build ./src/index.tsx ./src/embed.tsx --outdir ./lib --target=browser --sourcemap --minify --public-path ./ --define:process.env.NODE_ENV=\"production\" && cp index.html embed.html lib/",
    "dev": "bun dev.ts",
    "format": "prettier \"src/**/*.{ts,tsx,json,md}\" --write",
    "start": "bunx http-server ./lib/",
    "test": "echo 'No tests for this package'",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "preact": "^10.25.4",
    "preact-router": "^4.1.2",
    "websl": "*"
  },
  "devDependencies": {
    "@types/bun": "1.3.3",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.3"
}
